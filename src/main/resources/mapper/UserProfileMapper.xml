<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.app.ptever.mapper.UserProfileMapper">
    <select id="selectByUserId" resultType="userProfileVO">
        SELECT USER_ID, USER_PROFILE_PATH FROM TBL_USER_PROFILE
        WHERE USER_ID = #{userId}
    </select>
</mapper>